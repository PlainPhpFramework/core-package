{
    "name": "plainphp/core-package",
    "type": "plainphp-package",
    "bin": ["bin/plainphp-post-package-update.php"],
    "require": {
        "plainphp/package-installer": "dev-main@dev"
    },
    "autoload": {
        "classmap": [
            "lib/"
        ],
        "files": [
            "lib/functions.php"
        ]
    },
    "keywords": ["plainphp", "framework", "package"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}